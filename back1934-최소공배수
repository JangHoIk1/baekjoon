def gcd(a,b): # 유클리드 호제법 - 최대공약수 구하기
    if a%b==0:
        return b
    else:
        return gcd(b,a%b)
        
T= int(input())
gcd_num=0
chaso_num=0

for i in range(T):
    num1,num2 = map(int,input().split())
    gcd_num= gcd(num1,num2)
    print(num1*num2 // gcd_num) # 최소공배수는 두 수를 곱한 것에 최대공약수로 나누어 주면됨

